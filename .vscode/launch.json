{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "frontend (react)",
      "type": "chrome",
      "request": "attach",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/FRONTEND/src/components"
    },
    {
      "name": "backeend (python)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["api.main.app", "--reload"],
      "cwd": "${workspaceFolder}/BACKEND"
    }
  ]
}
